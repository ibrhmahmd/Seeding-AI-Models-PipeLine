# AI Model Seeding Pipeline Configuration
# Copy this file to .env and modify as needed

# Directories
DATA_DIR=./data
RAW_DATA_DIR=./data/raw
ENRICHED_DATA_DIR=./data/enriched
PROCESSED_DATA_DIR=./data/processed
MAPPED_DATA_DIR=./data/mapped
ARCHIVE_DIR=./archive
TAGS_DIR=./Tags

# Logging
LOG_LEVEL=INFO
LOG_FILE=./logs/pipeline.log
CONSOLE_LOG=true
FILE_LOG=true

# API Settings
API_URL=http://localhost:8000/api
API_KEY=your_api_key_here
API_TIMEOUT=30
API_RETRY_ATTEMPTS=3

# Extraction Settings
EXTRACT_BATCH_SIZE=10
EXTRACT_PARALLEL=false

# Enrichment Settings
ENRICHMENT_SOURCES=huggingface,custom
HUGGINGFACE_API_KEY=your_huggingface_key_here

# Tag Mapping Settings
TAG_MAP_FILE=./Tags/created_tag_ids.json
AUTO_CREATE_TAGS=false

# Model Mapping Settings
SCHEMA_VALIDATION=true
PLACEHOLDER_URL=https://example.com/placeholder

# Seeding Settings
SEED_BATCH_SIZE=5
SEED_PARALLEL=false
SEED_DELAY=1

# Archiving Settings
ARCHIVE_FORMAT=original
INCLUDE_TIMESTAMP=true
AUTO_CLEANUP=false
